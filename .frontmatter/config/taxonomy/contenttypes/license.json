{
    "$schema": "https://frontmatter.codes/config/taxonomy.contenttypes.schema.json",
    "$id": "https://github.com/dgmjr-io/dgmjr-io/.frontmatter/config/content/types/license.json",
    "title": "License",
    "name": "license",
    "description": "A license for a project",
    "pageBundle": false,
    "previewPath": null,
    "fields": [
        {
            "title": "Title",
            "name": "title",
            "type": "string",
            "single": true,
            "required": true
        },
        {
            "title": "Publish date",
            "name": "date",
            "type": "datetime",
            "default": "{{now}}",
            "isPublishDate": true,
            "single": true,
            "required": true
        },
        {
            "title": "Last Modified Date",
            "name": "lastmod",
            "type": "datetime",
            "default": "{{now}}",
            "isModifiedDate": true,
            "single": true,
            "required": true
        },
        {
            "title": "License",
            "name": "license",
            "type": "choice",
            "choices": [
                "agpl-3.0",
                "apache-2.0",
                "artistic-2.0",
                "bsl-1.0",
                "cc0-1.0",
                "epl-2.0",
                "gpl-3.0",
                "isc",
                "lgpl-3.0",
                "mit",
                "mpl-2.0",
                "ncsa",
                "nokia",
                "ofl-1.1",
                "osl-3.0",
                "sugarcrm-1.1.3",
                "unlicense",
                "wtfpl"
            ]
        }
    ],
    "template": "[[workspace]]/.frontmatter/templates/LICENSE.md",
    "defaultFleName": "LICENSE.md"
}
