				<element name="maxLength" id="maxLength" type="xs:numFacet">
					<annotation>
						<documentation source="http://www.w3.org/TR/xmlschema-2/#element-maxLength" />
					</annotation>
				</element>
				<element name="enumeration" id="enumeration" type="xs:noFixedFacet">
					<annotation>
						<documentation source="http://www.w3.org/TR/xmlschema-2/#element-enumeration" />
					</annotation>
				</element>
				<element name="whiteSpace" id="whiteSpace">
					<annotation>
						<documentation source="http://www.w3.org/TR/xmlschema-2/#element-whiteSpace" />
