					</restriction>
				</simpleType>
				<group name="simpleDerivation">
					<choice>
						<element ref="xs:restriction" />
						<element ref="xs:list" />
						<element ref="xs:union" />
					</choice>
				</group>
				<simpleType name="simpleDerivationSet">
					<annotation>
						<documentation>
