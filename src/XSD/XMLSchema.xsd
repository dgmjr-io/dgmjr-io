					<annotation>
						<documentation source="http://www.w3.org/TR/xmlschema-2/#element-fractionDigits" />
					</annotation>
				</element>
				<element name="length" id="length" type="xs:numFacet">
					<annotation>
						<documentation source="http://www.w3.org/TR/xmlschema-2/#element-length" />
					</annotation>
				</element>
				<element name="minLength" id="minLength" type="xs:numFacet">
					<annotation>
						<documentation source="http://www.w3.org/TR/xmlschema-2/#element-minLength" />
